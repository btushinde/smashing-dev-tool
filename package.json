  "author": "Biko Tushinde",
  "repository": {
    "type": "git",
    "url": "https://github.com/smashingboxes/smashing-dev-tool"
  },
  "description": "A frontend development toolkit for Smashing Boxes.",
  "usage": "[command] <cmd args>",
  "options": [
    {
      "option": "-a, --auto",
      "description": "auto-accept defaults where possible"
    },
    {
      "option": "-v, --verbose",
      "description": "display detailed logs and notifications"
    },
    {
      "option": "-d, --debug",
      "description": "show additional debug logs and notifications"
    },
    {
      "option": "-m, --mute",
      "description": "hide all logs and notifications"
    }
  ],
  "main": "./bin/smash",
  "bin": {
    "smash": "./bin/smash"
  },
